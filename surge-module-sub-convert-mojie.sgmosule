[Script]
rewrite_head = type=http-request,pattern=^https://mojie\.link/api/v1/client/subscribe\?token=* script-path=https://raw.githubusercontent.com/ieeeee/quan/main/surge/rewrite_request_request/mojie-request-headers.js,requires-body=true
